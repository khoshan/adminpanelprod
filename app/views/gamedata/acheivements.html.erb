<%= render "gamedata/menu" %>
<%= render "gamedata/menu_googlespreadsheet" %>
<table>
<tr>
<td><button type="submit" onclick="exportfileproduction(15, false, '');">Publish production</button></td>
</tr>
</table>

<div class="outer-container"> <!-- absolute positioned container -->
<div class="inner-container">

    <div class="table-header">
        <table id="headertable" width="200px" cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th class="header-cell">Type</th>
                    <th class="header-cell">Star</th>
                    <th class="header-cell">Name</th>
                    <th class="header-cell">Description</th>
                    <th class="header-cell">ExpReward</th>
                    <th class="header-cell">GemReward</th>
                    <th class="header-cell">RequiredQuantity</th>
                </tr>
            </thead>
        </table>
    </div>
    <div class="table-body">
<table class="ex1" style="font-size:13px" id="tblacheivements">
    <tbody>
     <% @datas.each_hash do |row|%>
        <tr>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achType"><%= row['Type']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achStar"><%= row['Star']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achName"><%= row['Name']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achDescription"><%= row['Description']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achExpReward"><%= row['ExpReward']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achGemReward"><%= row['GemReward']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="achRequiredQuantity"><%= row['RequiredQuantity']%></div></div></td>
        </tr>
     <% end%>
  </tbody>
</table>
</div>
</div>
</div>

