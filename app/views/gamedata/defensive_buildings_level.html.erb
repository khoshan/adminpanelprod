<%= render "gamedata/menu" %>
<%= render "gamedata/menu_googlespreadsheet" %>
<table>
<tr>
<td><button type="submit" onclick="exportfileproduction(5, false, '');">Publish production</button></td>
</tr>
</table>
<div class="outer-container"> <!-- absolute positioned container -->
<div class="inner-container">

    <div class="table-header">
        <table id="headertable" width="200px" cellpadding="0" cellspacing="0">
            <thead>
                <tr>
                    <th class="header-cell">Type</th>
                    <th class="header-cell">Level</th>
                    <th class="header-cell">DamagePerSecond</th>
                    <th class="header-cell">DamagePerShot</th>
                    <th class="header-cell">HitPoints</th>
                    <th class="header-cell">CostGold</th>
                    <th class="header-cell">CostDarkElixir</th>
                    <th class="header-cell">SellGold</th>
                    <th class="header-cell">SellDarkElixir</th>
                    <th class="header-cell">UpgradeTime</th>
                    <th class="header-cell">ExperienceGained</th>
                    <th class="header-cell">TownHallLevelRequired</th>
                    <th class="header-cell">ObjectType</th>
                    <th class="header-cell">CostToLoad</th>
                    <th class="header-cell">PrefabName</th>
                    <th class="header-cell">DisplayName</th>
                    <th class="header-cell">NumTileOneSide</th>
                    <th class="header-cell">HasBase</th>
                </tr>
            </thead>
        </table>
    </div>
    <div class="table-body">
     <table class="ex1" style="font-size:13px" id="tbldefensive_buildings_level">
    <tbody>
     <% @datas.each_hash do |row|%>
        <tr>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblType"><%= row['Type']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblLevel"><%= row['Level']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblDamagePerSecond"><%= row['DamagePerSecond']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblDamagePerShot"><%= row['DamagePerShot']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblHitPoints"><%= row['HitPoints']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblCostGold"><%= row['CostGold']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblCostDarkElixir"><%= row['CostDarkElixir']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblSellGold"><%= row['SellGold']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblSellDarkElixir"><%= row['SellDarkElixir']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblUpgradeTime"><%= row['UpgradeTime']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblExperienceGained"><%= row['ExperienceGained']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblTownHallLevelRequired"><%= row['TownHallLevelRequired']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblObjectType"><%= row['ObjectType']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblCostToLoad"><%= row['CostToLoad']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblPrefabName"><%= row['PrefabName']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblDisplayName"><%= row['DisplayName']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblNumTileOneSide"><%= row['NumTileOneSide']%></div></div></td>
          <td class="body-cell"><div><input type="hidden" value="<%= row['pkID']%>"/><div contenteditable class="dblHasBase"><%= row['HasBase']%></div></div></td>
        </tr>
     <% end%>
  </tbody>
</table>
</div>
</div>
</div>

